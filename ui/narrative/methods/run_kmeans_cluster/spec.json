    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "" ],
    "field_type" : "text",
    "text_options" : {
      "valid_ws_types" : ["KBaseFeatureValues.ExpressionMatrix", "KBaseFeatureValues.DifferentialExpressionMatrix",
